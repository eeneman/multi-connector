{
    "plugins": [
      "soap-basic",
      "fidelix-2"
      
    ],
    "protocol": "soap",
    "authConfig": {
      "url": "http://${ipAddress}/WebServices/Fidelix.asmx",
      "username": "${username}",
      "password": "${password}",
      "headers": "${headers}",
      "wsdl": "${wsdl}",
      "function": "Fidelix.FidelixSoap.setPointData",
      "path": {
         "point_id":"${processTargetId}",
         "state":"${state}",
         "val":"${processValue}"
        }
    },
    "dataObjects": [
      ""
    ],
    "dataPropertyMappings": {
      "${type}": "setPointDataResult"
    },
    "input": {
      "required": [
          "parameters.targetObject"
      ]
  },
    "generalConfig": {
      "hardwareId": {
        "dataObjectProperty": "hardwareId"
      },
      "timestamp": {
        "dataObjectProperty": "timestamp"
      }
    },
    "output": {
      "contextValue": "https://standards.oftrust.net/v2/Context/DataProductContext/Sensor/",
      "array": "sensors",
      "object": "data",
      "value": "result",
      "data": "measurements",
      "id": "id"
    }
  }
  