{
  "plugins": [
    "soap-ntlm",
    "fidelix"
  ],
  "protocol": "soap",
  "authConfig": {
    "url": "https://${ipAddress}/WebServices/Fidelix.asmx?WSDL",
    "username": "${username}",
    "password": "${password}",
    "headers": "${headers}",
    "wsdl": "${wsdl}",
    "function": "Fidelix.FidelixSoap.getPointDataList",
    "path": "${id}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "${type}": "getPointDataResult"
  },
  "generalConfig": {
    "hardwareId": {
      "dataObjectProperty": "hardwareId"
    }
  }
}
