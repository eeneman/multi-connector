{
    "plugins": [
        "sensor"
    ],
    "protocol": "custom",
    "authConfig": {
        "url": "${url}",
        "authPath": "api/v2.0/ClientToken",
        "authContentType": "application/json",
        "email": "${email}",
        "password": "${password}",
        "headers": {
            "Authorization": ""
        },
        "grantType": "${grantType}",
        "format": "${format}",
        "path": "api/v2.0/GetConsumptionsByCategory?Building=${id}&startTime=${startTime}&endTime=${endTime}&EnergyTypeIDs=${EnergyTypeIDs}&TimeGrouping=${TimeGrouping}&ShowMetaData=${ShowMetaData}&MeasurementSystem=${MeasurementSystem}&format=${format}"
    },
    "dataObjects": [""],
    "measurementType": {
        "1": "ElectricityConsumption",
        "2": "HeatConsumption",
        "3": "OtherConsumption"
    },
    "dataPropertyMappings": {
        "type": "EnergyTypeID",
        "value": "Consumption",
        "GroupDescription": "GroupDescription",
        "InformationID": "InformationID",
        "Unit": "Unit"
    },
    "generalConfig": {
        "query": {},
        "sourceName": {
            "dataObjectProperty": "name"
        },
        "hardwareId": {
            "dataObjectProperty": "BuildingStructureID"
        },
        "measurement": {
            "pathIndex": 5
        },
        "timestamp": {
            "dataObjectProperty": "Date"
        }
    },
    "input": {
        "required": []
    },
    "output": {
        "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/Sensor/",
        "timestamp": "timestamp",
        "array": "sensors",
        "object": "data",
        "value": "value",
        "data": "measurements",
        "id": "id",
        "type": "@type"
    }
}