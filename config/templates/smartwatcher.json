{
  "plugins": [
    "smartwatcher"
  ],
  "protocol": "rest",
  "authConfig": {
    "url": "${url}",
    "authPath": "api/auth",
    "authContentType": "application/json",
    "email": "${email}",
    "password": "${password}",
    "headers": {
      "Authorization": ""
    },
    "path": "api/v3/monitor/room/${id}/${dataType}?startDate=${startTime}&endDate=${endTime}"
  },
  "dataObjects": [],
  "measurementType": "",
  "dataPropertyMappings": {
    "type": "value",
    "MeasureAirTemperatureCelsiusDegree": "temperature",
    "MeasureAirHumidityPercent": "humidity"
  },
  "generalConfig": {
    "query": {},
    "sourceName": {
      "dataObjectProperty": "name"
    },
    "hardwareId": {
      "pathIndex": 4
    },
    "measurement": {
      "pathIndex": 5
    },
    "timestamp": {
      "dataObjectProperty": "timestamp"
    }
  },
  "input": {
    "required": [
      "parameters.startTime",
      "parameters.endTime"
    ]
  },
  "output": {
    "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/Sensor/",
    "timestamp": "timestamp",
    "array": "sensors",
    "object": "data",
    "value": "value",
    "data": "measurements",
    "id": "id"
  }
}