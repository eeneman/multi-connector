{
    "plugins": [
        "parma-product-catalog-ncc-update"
    ],
    "protocol": "ftp",
    "authConfig": {
        "url": "${url}",
        "port": "${port}",
        "secure": "${secure}",
        "fromPath": "${fromPath}",
        "toPath": "${toPath}",
        "username": "${username}",
        "password": "${password}",
        "path": "/${targetObject.name}"
    },
    "dataObjects": [
        "data"
    ],
    "dataPropertyMappings": {
        "": ""
    },
    "generalConfig": {
        "hardwareId": {
            "dataObjectProperty": ""
        },
        "sourceName": {
            "dataObjectProperty": ""
        },
        "timestamp": {
            "dataObjectProperty": ""
        }
    },
    "input": {
        "required": [
            "parameters.targetObject.name"
        ]
    },
    "output": {
        "contextValue": "https://standards-ontotest.oftrust.net/v2/DataExample/DataProductParameters/ProductCatalog",
        "array": "product"
    }
}
